<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Success Page</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Thank you for your purchase!</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Sapient\Worldpay\Block\Multishipping\Paybylink\Success" name="checkout_success" template="Sapient_Worldpay::multishipping/paybylink/success.phtml" cacheable="false">
                <arguments>
                    <argument name="checkout_data" xsi:type="object">Magento\Multishipping\Block\DataProviders\Success</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
